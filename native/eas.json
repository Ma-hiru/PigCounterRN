{
  "cli": {
    "version": ">= 15.0.14",
    "requireCommit": true
  },
  "build": {
    "preInstallCommand": "pnpm i -g pnpm@10",
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "node": "23.10.0",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
